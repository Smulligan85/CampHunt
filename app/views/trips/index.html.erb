<h1>All Your Trips</h1>
<div class="col-md-6">
    <table class="table table-striped">
      <tr>
        <th>Trip Name</th>
        <th>Start Date</th>
        <th>End Date</th>
      </tr>
      <% @trips.each do |trip| %>
        <tr>
          <td><%= link_to "#{trip.name}", trip_path(trip) %></td>
          <td><%= trip.start_date_formatter %></td>
          <td><%= trip.end_date_formatter %></td>
        </tr>
      <% end %>
    </table>
</div>
