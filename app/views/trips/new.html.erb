<h2>Lets get ready for your trip to <%= @name %></h2>
<%= form_for @trip do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name, :value => @name %>
  <%= f.label "Start Date" %>
  <%= f.date_field :start_date %>
  <%= f.label "End Date" %>
  <%= f.date_field :end_date %>
  <%= f.submit %>
<% end %>
